{
  "name": "resourceful-redux-project",
  "private": true,
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --compilers js:babel-register --colors --globals expect packages/resourceful-redux/test/setup/node.js packages/resourceful-redux/test/unit/**",
    "lint": "gulp lint",
    "watch": "gulp watch",
    "build": "gulp clean && gulp build && NODE_ENV=production gulp build",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "bootstrap": "lerna bootstrap",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:prepare": "gitbook install",
    "docs:clean": "rimraf _book",
    "docs:build": "npm run docs:prepare && gitbook build",
    "docs:publish": "npm run docs:clean && npm run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:jmeas/resourceful-redux gh-pages --force"
  },
  "license": "MIT",
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-polyfill": "6.13.0",
    "babel-preset-latest": "6.14.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "6.14.0",
    "chai": "3.5.0",
    "cross-env": "^5.0.1",
    "del": "2.2.2",
    "gitbook-cli": "^2.3.0",
    "gitbook-plugin-ga": "^2.0.0",
    "glob": "7.0.6",
    "gulp": "3.9.1",
    "gulp-eslint": "3.0.1",
    "gulp-filter": "4.0.0",
    "gulp-istanbul": "1.1.2",
    "gulp-livereload": "3.8.1",
    "gulp-load-plugins": "1.2.4",
    "gulp-mocha": "3.0.1",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "1.2.2",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "2.0.0",
    "in-publish": "^2.0.0",
    "isparta": "4.0.0",
    "json-loader": "0.5.4",
    "lerna": "^2.0.0-rc.5",
    "lodash": "^4.17.4",
    "mocha": "^3.0.2",
    "nyc": "^11.0.3",
    "pleaserc": "^2.3.1",
    "run-sequence": "^2.0.0",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "webpack": "2.6.1",
    "webpack-stream": "^3.2.0"
  }
}
